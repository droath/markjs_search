<?php

/**
 * @file
 * The hook implementations for the MarkJS search module.
 */

/**
 * Implements hook_theme().
 */
function markjs_search_theme($existing, $type, $theme, $path) {
  return [
    'markjs_search_box' => [
      'render element' => 'element',
      'template' => 'markjs-search-box',
      'path' => $path . '/templates',
    ]
  ];
}
